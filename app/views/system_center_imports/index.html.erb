<div>
  <%= button_to "Import Compliance Report", import_system_center_path, :data => {:confirm => 'Are you sure you want to import new data?'} %>
</div>

<br>
<%= @report_2.name %>
<div class="container-fluid">
  <table>
    <thead>
      <tr>
        <th>Category</th>
        <th>Targeted</th>
        <th>Failed</th>
        <th>Unknown</th>
        <th>Non-Compliant</th>
        <th>Compliant</th>
        <th>Collection</th>
        <th>Compliance</th>
        <th>Baseline Revision</th>
        <th>Baseline Name</th>
      </tr>
    </thead>

    <tbody>
      <% @imports.each do |import| %>
        <tr>
          <td><%= import.category %></td>
          <td><%= number_with_delimiter(import.targeted_count, :delimiter => ',') %></td>
          <td><%= number_with_delimiter(import.failed_count, :delimiter => ',') %></td>
          <td><%= number_with_delimiter(import.unknown_count, :delimiter => ',') %></td>
          <td><%= number_with_delimiter(import.non_compliant_count, :delimiter => ',') %></td>
          <td><%= number_with_delimiter(import.compliant_count, :delimiter => ',') %></td>
          <td><%= import.collection_name %></td>
          <td><%= number_to_percentage(import.compliance_percentage, precision: 0)%></td>
          <td><%= import.configuration_baseline_revision %></td>
          <td><%= import.configuration_baseline_name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<div>
  <%= button_to "Import Patch Report", import_system_center_2_path, :data => {:confirm => 'Are you sure you want to import new data from report 2?'} %>
</div>

<br>
<%= @report_1.name %>
<div class="container-fluid">
  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Average Patch Duration</th>
      </tr>
    </thead>

    <tbody>
      <% @imports_2.each do |import| %>
        <tr>
          <td><%= import.created_at %></td>
          <td><%= import.average_patch_duration_days %> Days</td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<br>

<% @servicenow %>