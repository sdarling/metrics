<p id="notice"><%= notice %></p>

<h1>Listing Sccm Metrics</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Summary</th>
      <th># Projects</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sccm_metrics.each do |sccm_metric| %>
      <tr>
        <td><%= sccm_metric["Title"] %></td>
        <td><%= sccm_metric["Summary"] %></td>
        <td><%= sccm_metric["ProjectCount"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

