<%= simple_form_for(@software_center) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :category %>
    <%= f.input :targeter %>
    <%= f.input :failed %>
    <%= f.input :unknown %>
    <%= f.input :non_compliant %>
    <%= f.input :compliant %>
    <%= f.input :collection_name %>
    <%= f.input :compliance_percentage %>
    <%= f.input :target_compliant_percentage %>
    <%= f.input :configuration_baseline_revision %>
    <%= f.input :conviguration_baseline_name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
