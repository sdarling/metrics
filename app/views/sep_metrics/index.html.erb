<p id="notice"><%= notice %></p>

<h1>Listing Sep Metrics</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sep_metrics.each do |sep_metric| %>
      <tr>
        <td><%= link_to 'Show', sep_metric %></td>
        <td><%= link_to 'Edit', edit_sep_metric_path(sep_metric) %></td>
        <td><%= link_to 'Destroy', sep_metric, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sep metric', new_sep_metric_path %>
